{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "browser": true,
        "es6": true 
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-case-declarations": 0,
        "no-console": 1,
        "react/prop-types": 0,
        "react/no-unescaped-entities": 0
    },
    "globals": {
        "__DEV__": true
    }
}